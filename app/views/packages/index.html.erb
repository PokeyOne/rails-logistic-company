<h1>Packages</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>From Address</th>
    <th>To Address</th>
  </tr>
  <% @packages.each do |package| %>
    <tr>
      <td><%= package.id %></td>
      <td><%= package.name %></td>
      <td><%= package.from_address.one_liner %></td>
      <td><%= package.to_address.one_liner %></td>
    </tr>
  <% end %>
</table>